{
  "name": "duckdb-wasm-parquet",
  "version": "0.1.0",
  "description": "DuckDB-WASM Parquet Viewer with MoonBit",
  "type": "module",
  "homepage": "https://f4ah6o.github.io/duckdb_wasm_parquet.mbt/",
  "scripts": {
    "dev": "vite",
    "build": "pnpm moon:build && vite build",
